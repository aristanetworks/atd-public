   name LEAF_PEER_L3
   trunk group LEAF_PEER_L3
!
vlan 4094
   name MLAG_PEER
   trunk group MLAG
!
interface Port-Channel47
   description MLAG_PEER_leaf-1a_Po47
   switchport mode trunk
   switchport trunk group LEAF_PEER_L3
   switchport trunk group MLAG
!
interface Ethernet1
   description IDF1 Standard Voice Port
   switchport access vlan 120
   spanning-tree portfast
!
interface Ethernet47
   description MLAG_PEER_leaf-1a_Ethernet47
   channel-group 47 mode active
!
interface Ethernet48
   description MLAG_PEER_leaf-1a_Ethernet48
   channel-group 47 mode active
!
interface Ethernet49
   description P2P_LINK_TO_SPINE-2_Ethernet3
   mtu 1500
   no switchport
   ip address 10.0.0.7/31
!
interface Loopback0
   description Management and Router-id
   ip address 10.255.0.6/32
!
interface Vlan110
   description IDF1-Data
   no autostate
