interface Ethernet1
   shutdown
!
interface Ethernet2
   description L3 Downlink - s1-leaf1
   no switchport
   ip address 10.111.2.0/31
   ip ospf area 0.0.0.0
   ip ospf authentication message-digest
   ip ospf message-digest-key 1 sha512 7 UnIJI4WaG2JZQQTx/pfvsg==
!
interface Ethernet3
   description L3 Downlink - s1-leaf2
   no switchport
   ip address 10.111.2.2/31
   ip ospf area 0.0.0.0
   ip ospf authentication message-digest
   ip ospf message-digest-key 1 sha512 7 UnIJI4WaG2JZQQTx/pfvsg==
!
interface Ethernet4
   description L3 Downlink - s1-leaf3
   no switchport
   ip address 10.111.2.4/31
   ip ospf area 0.0.0.0
   ip ospf authentication message-digest
   ip ospf message-digest-key 1 sha512 7 UnIJI4WaG2JZQQTx/pfvsg==
!
interface Ethernet5
   description L3 Downlink - s1-leaf4
   no switchport
   ip address 10.111.2.6/31
   ip ospf area 0.0.0.0
   ip ospf authentication message-digest
   ip ospf message-digest-key 1 sha512 7 UnIJI4WaG2JZQQTx/pfvsg==
!
interface Ethernet6
   shutdown
!
interface Loopback0
   description Management and Router-id
   ip address 10.111.0.2/32
   ip ospf area 0.0.0.0
!
router ospf 100
   router-id 10.111.0.2
   passive-interface default
   no passive-interface Ethernet2
   no passive-interface Ethernet3
   no passive-interface Ethernet4
   no passive-interface Ethernet5
   max-lsa 12000