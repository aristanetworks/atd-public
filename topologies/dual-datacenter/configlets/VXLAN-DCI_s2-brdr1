no spanning-tree vlan-id 4094
!
vlan 112
   name Host_Network_112
!
vlan 134
   name Host_Network_134
!
vlan 4094
   name MLAG_VLAN
   trunk group MLAGPEER
!
interface Port-Channel1
   description MLAG Peer-link - s2-brdr2
   switchport mode trunk
   switchport trunk group MLAGPEER
!
interface Port-Channel56
   description MLAG Uplink - s2-spine1 and s2-spine2
   switchport mode trunk
   mlag 56
!
interface Ethernet1
   description MLAG Peer-link - s2-brdr2
   channel-group 1 mode active
!
interface Ethernet2
   description MLAG Uplink - s2-spine1
   channel-group 56 mode active
!
interface Ethernet3
   description MLAG Uplink - s2-spine2
   channel-group 56 mode active
!
interface Ethernet4
   description L3 Uplink - s2-core1
   no switchport
   ip address 10.222.99.1/31
!
interface Ethernet5
   description L3 Uplink - s2-core2
   no switchport
   ip address 10.222.99.129/31
!
interface Ethernet6
   description MLAG Peer-link - s2-brdr2
   channel-group 1 mode active
!
interface Loopback0
   description Management and Router-id
   ip address 10.222.254.5/32
!
interface Loopback1
   description VTEP
   ip address 10.222.253.5/32
!
interface Vlan4094
   description MLAG Peer Network
   ip address 10.255.255.1/30
!
interface Vxlan1
   vxlan source-interface Loopback1
   vxlan udp-port 4789
   vxlan vlan 112 vni 112
   vxlan vlan 134 vni 134
   vxlan flood vtep 10.111.253.5
!
mlag configuration
   domain-id MLAG
   local-interface Vlan4094
   peer-address 10.255.255.2
   peer-link Port-Channel1
!
mac address-table static 001c.7300.0012 vlan 112 interface Port-Channel56
mac address-table static 001c.7300.0012 vlan 134 interface Port-Channel56
!
ip prefix-list LOOPBACKS
   seq 100 permit 10.222.253.0/24 eq 32
   seq 110 permit 10.222.254.0/24 eq 32
!
route-map LOOPBACKS permit 100
   match ip address prefix-list LOOPBACKS
!
router bgp 65103
   router-id 10.222.254.5
   maximum-paths 2
   neighbor MLAG peer group
   neighbor MLAG remote-as 65103
   neighbor MLAG next-hop-self
   neighbor MLAG send-community standard extended
   neighbor CORE peer group
   neighbor CORE remote-as 65001
   neighbor CORE send-community standard extended
   neighbor 10.222.99.0 peer group CORE
   neighbor 10.222.99.128 peer group CORE
   neighbor 10.255.255.2 peer group MLAG
   redistribute connected route-map LOOPBACKS